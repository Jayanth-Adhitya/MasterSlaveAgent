# ===========================================
# Agent Prototype - Environment Configuration
# ===========================================
# Copy this file to .env and update the values

# Domain Configuration (for Coolify/Traefik)
DOMAIN=your-domain.com

# PostgreSQL Database
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your-secure-password-here
POSTGRES_DB=agent_db

# JWT Authentication (CHANGE IN PRODUCTION!)
JWT_SECRET_KEY=generate-a-secure-random-string-at-least-32-chars
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=60

# Google Gemini API Key (Required for AI features)
GOOGLE_API_KEY=your-gemini-api-key-here

# Application Settings
APP_NAME=Agent Prototype
DEBUG=false
